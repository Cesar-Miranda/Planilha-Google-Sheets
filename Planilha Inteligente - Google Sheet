AND(F7="3A";E7/H7>TIMEVALUE("00:02");G7<=200;G7>=61);"X";
AND(F7="3A";E7/H7<TIMEVALUE("00:02");E7/H7>TIMEVALUE("00:01");G7<=200;G7>=61);"OK";
AND(F7="2A";E7/H7<TIMEVALUE("00:01"));"A";
AND(F7="2A";E7/H7>TIMEVALUE("00:02"));"X";
